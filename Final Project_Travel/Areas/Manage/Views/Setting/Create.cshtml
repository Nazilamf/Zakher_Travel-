﻿﻿
@model Setting

@{
    ViewData["Title"] = "Create";
}

<div class="container">
    <div class="row justify-content-center">
        <div class="col-md-8">
            <h1>Setting Create</h1>
        </div>
        <div class="col-md-8">
            <form method="post" asp-action="create">
                <div class="form-group">
                    <label asp-for="Key">Key</label>
                    <input asp-for="Key" class="form-control">
                    <span class="text-danger" asp-validation-for="Key"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Value">Value</label>
                    <input asp-for="Value" class="form-control">
                    <span class="text-danger" asp-validation-for="Value"></span>
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>

            </form>
        </div>

    </div>
</div>